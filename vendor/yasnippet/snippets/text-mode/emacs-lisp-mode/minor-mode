#name : measure time
#contributor : Bartuer
# --
(defvar ${1:name}-mode-map
  (let ((map (make-sparse-keymap)))
    map)
  "Minor mode keymap for $1-mode")

(define-minor-mode $1-mode
  :group 'org :lighter " ${2:mode-line} " :keymap $1-mode-map
  (cond ($1-mode
          $0
         )
        (t
         
         ))
  )
